f1 = figure('Color', 'w', 'units', 'normalized', 'Position', [.1 .1 .6 .9]);
rightGo_hitStd = nanstd(rightGo_performance)/sqrt(20);
randoGo_hitStd = nanstd(randoGo_performance)/sqrt(20);

plot([-100 100], [.5 .5], 'Color', 'k')
xlim([.75 4.25])
ylim([0 1.1])

hold on

z1 = scatter(1:4, nanmean(rightGo_performance), 200)
set(z1, 'MarkerFaceColor', 'none', 'MarkerEdgeColor', 'r', 'LineWidth', 4)
hold on
errorbar( nanmean(rightGo_performance), rightGo_hitStd, 'r', 'LineWidth', 4)

z2 = scatter(1:4, nanmean(randoGo_performance), 200)
set(z2, 'MarkerFaceColor', 'none', 'MarkerEdgeColor', 'k', 'LineWidth', 4)
hold on
errorbar( nanmean(randoGo_performance), randoGo_hitStd, 'k', 'LineWidth', 4)

set(gca, 'XTick', [1 2 3 4], 'XTickLabel', {'train block 1' 'test block 1' 'train block 2' 'test block 2'}, 'YTick', [.25 .5 .75 1])
ylabel('found cool zone / chose right')

box off

title('train to right summary')

