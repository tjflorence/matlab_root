%% experiment flies
rightGo_insertVec           = 7*ones(length(rightGo),42);
rightGo_insertVec_spread    = rightGo_insertVec;
rightGo_performance         = nan(length(rightGo), 4);
rightGo_fracCool            = nan(length(rightGo), 28);

for aa = 1:length(rightGo)
    
    cd(rightGo(aa).tr1)
    try
        load('summaryData.mat')
    catch
        addVarStats_tetheredMaze_prePhases(rightGo(aa).tr1)
        addVarStats_tetheredMaze_prePhases(rightGo(aa).tr2)
        cd(rightGo(aa).tr1)
        load('summaryData.mat')
    end

    rightGo_insertVec(aa,1:10)     = summaryData.outcomeVec(1:10);
    rightGo_insertVec(aa,12:15)    = summaryData.outcomeVec(11:14);
    
    rightGo_insertVec_spread = rightGo_insertVec;
    
    trainFracFound = length(find(summaryData.outcomeVec==1))/10;
    testFracFound  = length(find(summaryData.outcomeVec==3))/4;
    
    rightGo_performance(aa,1:2) = [trainFracFound testFracFound];
    rightGo_fracCool(aa,1:14)   = summaryData.fracTimeCold;
   
    cd(rightGo(aa).tr2)
    load('summaryData.mat')
    rightGo_insertVec(aa,17:26)     = summaryData.outcomeVec(1:10);
    rightGo_insertVec(aa,28)        = summaryData.outcomeVec(11);
    rightGo_insertVec(aa,29)        = summaryData.outcomeVec(12);
    rightGo_insertVec(aa,30)        = summaryData.outcomeVec(13);
    rightGo_insertVec(aa,31)        = summaryData.outcomeVec(14);
    
    rightGo_insertVec_noSpread(17:26) = summaryData.outcomeVec(1:10);
    rightGo_insertVec_noSpread(28:31) = summaryData.outcomeVec(11:14);
    
    trainFracFound = length(find(summaryData.outcomeVec==1))/10;
    testFracFound  = length(find(summaryData.outcomeVec==3))/4;
    
    rightGo_performance(aa,3:4) = [trainFracFound testFracFound];
    rightGo_fracCool(aa,15:28)   = summaryData.fracTimeCold;
end

%% leftGo
leftGo_insertVec           = 7*ones(length(leftGo),42);
leftGo_insertVec_spread    = leftGo_insertVec;
leftGo_performance         = nan(length(leftGo), 4);
leftGo_fracCool            = nan(length(leftGo), 28);

for aa = 1:length(leftGo)
    
    cd(leftGo(aa).tr1)
    try
        load('summaryData.mat')
    catch
        addVarStats_tetheredMaze_prePhases(leftGo(aa).tr1)
        addVarStats_tetheredMaze_prePhases(leftGo(aa).tr2)
        cd(leftGo(aa).tr1)
        load('summaryData.mat')
    end

    leftGo_insertVec(aa,1:10)     = summaryData.outcomeVec(1:10);
    leftGo_insertVec(aa,12:15)    = summaryData.outcomeVec(11:14);
    
    leftGo_insertVec_spread = leftGo_insertVec;
    
    trainFracFound = length(find(summaryData.outcomeVec==1))/10;
    testFracFound  = length(find(summaryData.outcomeVec==2))/4;
    
    leftGo_performance(aa,1:2) = [trainFracFound testFracFound];
    leftGo_fracCool(aa,1:14)   = summaryData.fracTimeCold;
   
    cd(leftGo(aa).tr2)
    load('summaryData.mat')
    leftGo_insertVec(aa,17:26)     = summaryData.outcomeVec(1:10);
    leftGo_insertVec(aa,28)        = summaryData.outcomeVec(11);
    leftGo_insertVec(aa,29)        = summaryData.outcomeVec(12);
    leftGo_insertVec(aa,30)        = summaryData.outcomeVec(13);
    leftGo_insertVec(aa,31)        = summaryData.outcomeVec(14);
    
    leftGo_insertVec_spread(17:26) = summaryData.outcomeVec(1:10);
    leftGo_insertVec_spread(28:31) = summaryData.outcomeVec(11:14);
    
    trainFracFound = length(find(summaryData.outcomeVec==1))/10;
    testFracFound  = length(find(summaryData.outcomeVec==2))/4;
    
    leftGo_performance(aa,3:4) = [trainFracFound testFracFound];
    leftGo_fracCool(aa,15:28)   = summaryData.fracTimeCold;
end

